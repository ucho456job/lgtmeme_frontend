{
  "name": "lgtmeme",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "prepare": "panda codegen && npm run prisma:generate",
    "dev": "rm -rf .next && next dev -p 3002",
    "dev:start": "bash ./shells/start_development.sh",
    "docker:start": "docker compose -f docker-compose.local.yml up -d",
    "docker:stop": "docker compose -f docker-compose.local.yml down",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write --ignore-path .gitignore '**/*.{js,jsx,ts,tsx,json}' && prisma format",
    "type-check": "tsc --noEmit -p ./tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "db:seed": "node ./prisma/createData.js",
    "db:clear": "node ./prisma/clearData.js",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "e2e:docker:start": "docker compose -f e2e/docker-compose.e2e.yml up -d",
    "e2e:docker:stop": "docker compose -f e2e/docker-compose.e2e.yml down",
    "e2e": "bash ./shells/run_e2e.sh",
    "release:major": "bash ./shells/release.sh major",
    "release:minor": "bash ./shells/release.sh minor",
    "release:patch": "bash ./shells/release.sh patch",
    "delete:branch": "git fetch -p && git branch -vv | grep ': gone]' | awk '{print $1}' | xargs git branch -D",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@supabase/supabase-js": "^2.36.0",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "base64-arraybuffer": "^1.0.2",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "jsonwebtoken": "^9.0.2",
    "next": "^13.5.2",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.17.5",
    "@storybook/addon-essentials": "^7.4.3",
    "@storybook/addon-interactions": "^7.4.3",
    "@storybook/addon-links": "^7.4.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.3",
    "@storybook/nextjs": "^7.4.3",
    "@storybook/react": "^7.4.3",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/selenium-webdriver": "^4.1.18",
    "@types/uuid": "^9.0.4",
    "chromedriver": "^118.0.1",
    "eslint-plugin-storybook": "^0.6.14",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.4",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "selenium-webdriver": "^4.14.0",
    "storybook": "^7.4.3",
    "ts-jest": "^29.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css,scss,md}": [
      "npm run format"
    ]
  }
}
